#!/bin/bash

function colors {
  GREEN="\e[32m"
  RED="\e[39m"
  NORMAL="\e[0m"
  LMAGENTA='\e[1;35m' 
}

colors



echo -e '\e[90me[1;35m'
echo -e '                                                   '
echo -e '  /$$$$$$  /$$$$$$   /$$   /$$$$$$$$$$$ /$$      /$$'
echo -e ' /$$__  $$| $$__ $$ | $$  |___  $$___/ | $$$    /$$$'
echo -e '| $$  \__/| $$  \ $$| $$      | $$     | $$$$  /$$$$'
echo -e '| $$      | $$  | $$| $$      | $$     | $$ $$/$| $$'
echo -e '| $$   /$$| $$$$$$ /| $$      | $$     | $$  $$ | $$'
echo -e '| $$  | $$| $$ __ / | $$      | $$     | $$     | $$'
echo -e '|  $$$$$$/| $$      | $$ /$$  | $$     | $$     | $$'
echo -e ' \______/ |__/      |__/ |_/  |__/     |__/     |__/'
echo -e '                                                    '
echo -e '\e[0m'
